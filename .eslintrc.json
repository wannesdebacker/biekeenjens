{
  "extends": ["next", "plugin:prettier/recommended"],
  "rules": {
    "@next/next/no-img-element": "off",
    "jsx-a11y/alt-text": [ 2, {
      "elements": [ "img", "object", "area", "input[type=\"image\"]" ],
      "object": ["Object"],
      "area": ["Area"],
      "input[type=\"image\"]": ["InputImage"]
    }]
  }
}